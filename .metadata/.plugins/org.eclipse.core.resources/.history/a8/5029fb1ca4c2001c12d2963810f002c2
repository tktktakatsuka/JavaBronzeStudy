import java.util.Scanner;


public class Main {
    public static void main(String[] args) {
        // 自分の得意な言語で
        // Let's チャレンジ！！
        Scanner sc = new Scanner(System.in);
        int line = sc.nextInt();
        System.out.println(line);
        
        for (int i = 1; i > line; i++){
            System.out.println(i);
        }
    }
}