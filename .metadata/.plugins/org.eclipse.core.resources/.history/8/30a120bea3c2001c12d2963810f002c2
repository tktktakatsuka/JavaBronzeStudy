import java.util.Scanner;


public class Main {
    public static void main(String[] args) {
        // 自分の得意な言語で
        // Let's チャレンジ！！
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        
        
        if(num == 1){
          System.out.println(1);
        }else if( num == 2){
        System.out.println(1);
        System.out.println(2);    
        }
        
    }
}