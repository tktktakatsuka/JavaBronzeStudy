package com.example.factory;

 class Factory {

	//productを生成して登録するメソッド
	public static Product create(String param) {
		if(param.equals("NEC")) {
			return new NecPersonalcomputer();
			
		}else if(param.equals("FUJITU")) {
			return new FujituPersonalComputer();
			
		}else
			return new DellPersonalComputer();

	}
	
}
	
public static void main(String[] args)	{
	Factory.create("NEC");



}
